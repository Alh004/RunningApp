@page
@model Create

@{
    Layout = null; // or your layout file
}

   

<h2>Create New Member</h2>

<form method="post">
    <div class="form-group">
        <label asp-for="NewPerson.Navn">Name</label>
        <input asp-for="NewPerson.Navn" class="form-control" required />
        <span asp-validation-for="NewPerson.Navn" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="NewPerson.Team">Team</label>
        <input asp-for="NewPerson.Team" class="form-control" required />
        <span asp-validation-for="NewPerson.Team" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-primary">Create</button>
</form>
